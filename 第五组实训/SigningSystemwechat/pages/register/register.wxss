
#registerMap{
  width: 100%;
  height:79%;
  position: absolute;
  top:0;
}
.signIn{
  position: absolute;
  left: 40%;
  bottom: 5%;
  border-radius: 50% 50%;
  padding: 2% 4%;
  z-index: 1000;
  background-color: blue;
  color: white;
}
.edit, .list, .inform{
  position: fixed;
  bottom: 5%;
  width: 8%;
  height: 5%;
  padding:2% 2%;
  background-color: white;
  border-radius: 50% 50%;
  z-index: 1000;
}
.edit{
  left: 2%;
}
.editImage{
  width: 80%;
  height: 80%;
  position: absolute;
  left: 10%;
  top:10%;
}
.list, .inform{
  right: 2%;
}
.inform{
  bottom: 13%;
}